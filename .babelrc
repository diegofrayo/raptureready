{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    [
      "babel-plugin-transform-require-ignore",
      "transform-decorators-legacy",
      {
        "extensions": [".less", ".sass", ".css", ".scss", ".styl", ".png"]
      }
    ]
  ]
}